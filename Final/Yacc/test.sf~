function subtract(x, y)



			integer x, y

			
			integer subtract

			integer nomatter

			subtract = x - y


end function subtract

program example


	integer :: d ! real


	common /d/

	integer a, b, c ! integers


	integer :: e ! array with 10 integers


	integer, dimension(10), pointer :: p ! pointer to 10-integer array

	character :: str

	data / d / 20 /

	parameter (a = 10, b=2)


	interface ! interface for forward declarations




		



		subroutine multiply(x, y)



			integer x, y



		end subroutine multiply

		function add(w, z)



			integer w, z

			integer add

		end function add


	end interface


	c = e + add( 1,2)
	e = 200

	do b = 10, 20
		e = e + 2
	end do

	if( .TRUE. ) then
		e = 20
		if( .TRUE. ) then ! 82
			e = 3 ! 83
		else ! 84
			e = 4
		end if
	end if

	call multiply( 1, 2)

	d = 1 + 400

	print "hello "" world"
end program example

subroutine multiply(x, y)



			real x, y

			
			x = -x
			
			x = -x


end subroutine multiply

function add(w, z)



			integer w, z

			integer add

			
			w = -w

			w = -w
			
			add = w + z

end function add
